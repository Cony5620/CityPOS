<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CityPOS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CityPOS.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">CityPOS</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <partial name="_LoginPartial" />
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Category
                       
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Category" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Category" asp-action="list">List</a></li>
                               
                            </ul>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Brand

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Brand" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Brand" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Unit

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Unit" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Unit" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                City

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="City" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="City" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Township

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Township" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Township" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Supplier

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Supplier" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Supplier" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Customer

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Customer" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Customer" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Item

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Item" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Item" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Price

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Price" asp-action="Entry">Entry</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Price" asp-action="list">List</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Stock

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="StockBalance" asp-action="Entry">Stock Control</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="StockBalance" asp-action="list">Stock Balance List</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="StockLedger" asp-action="list">Stock Ledger</a></li>

                            </ul>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Purchase

                            </a>
                            <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Purchase" asp-action="Entry">New Purchase</a></li>
                                <li>  <a class="nav-link text-dark" asp-area="" asp-controller="Purchase" asp-action="list">Purchase List</a></li>
                               

                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - CityPOS - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
